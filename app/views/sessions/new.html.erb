<h1> Sign in :) </h1>
<div class="row">
  <div class="span6 offset3">
<%= form_for(:session, url: sessions_path) do |f|%>
<div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>
  <div class="actions">
    <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
  </div>
<% end %>

<p>New user ? <%= link_to "Sign up now!", signup_path %></p>

 </div>
  </div>